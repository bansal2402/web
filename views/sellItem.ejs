    </div>
      <section id="container" >
        <div id="left" class="pd-20">
          
        </div>
        
        
        <div id="middle" class="pd-20">
          
          <form action="/postProduct" method="POST" enctype="multipart/form-data" class="form_group">
          <label for="img">Add Image</label>
          <input type="file" multiple id="img" name="image" value="">

          <input type="hidden" multiple id="img2" name="image2" value="">
          
          <div id="preview">
          <h1>Uploaded Images</h1>


          
       


        </div>


            
              <input type="hidden" value="<%= id %>" name="id">
              <div class="form-group">
                <label for="" class="label-group">Price :</label>
                <input type="number" class="inputText" name="productPrice" placeholder="price">

            </div>
            <div class="form-group">
                <label for="" class="label-group">Product Name :</label>
                <input type="text" class="inputText" name="productName" placeholder="Product Name">

            </div>

            <div class="form-group">
              <label for="types" class="label-group">Product Type :</label>
              <select name="type" id="types" form="product-form-id" class="typeList">
                  <%typesList.forEach(function(type){%>
                      <option value="<%=type%>"><%=type%></option>
                  <%});%>
              </select>

          </div>
          <div class="form-group">
              <div><label for="" class="label-group">Product Description :</label></div>
              <textarea name="productDescription" id="" cols="30" rows="10" class="inputText textArea"
                  placeholder="Description" form="product-form-id"></textarea>

          </div>

          <div class="form-group">
            <!-- <input type="submit" class="submit-btn"> -->
            <button class="submit-btn">Post</button>
        </div>

            </form>

        </div>





        <div id="right" class="pd-20"></div>
      </section>


      </span>



